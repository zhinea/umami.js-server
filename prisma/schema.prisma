generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model event_data {
  event_data_id    String    @id @db.VarChar(36)
  website_event_id String    @db.VarChar(36)
  website_id       String    @db.VarChar(36)
  event_key        String    @db.VarChar(500)
  string_value     String?   @db.VarChar(500)
  number_value     Decimal?  @db.Decimal(19, 4)
  date_value       DateTime? @db.Timestamp(0)
  data_type        Int       @db.UnsignedInt
  created_at       DateTime? @default(now()) @db.Timestamp(0)

  @@index([created_at])
  @@index([website_event_id])
  @@index([website_id, created_at, event_key])
  @@index([website_id, created_at])
  @@index([website_id])
  @@index([website_id, website_event_id, created_at])
}

model report {
  report_id   String    @id @unique @db.VarChar(36)
  user_id     String    @db.VarChar(36)
  website_id  String    @db.VarChar(36)
  type        String    @db.VarChar(200)
  name        String    @db.VarChar(200)
  description String    @db.VarChar(500)
  parameters  String    @db.VarChar(6000)
  created_at  DateTime? @default(now()) @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)

  @@index([name])
  @@index([type])
  @@index([user_id])
  @@index([website_id])
}

model session {
  session_id   String    @id @unique @db.VarChar(36)
  website_id   String    @db.VarChar(36)
  hostname     String?   @db.VarChar(100)
  browser      String?   @db.VarChar(20)
  os           String?   @db.VarChar(20)
  device       String?   @db.VarChar(20)
  screen       String?   @db.VarChar(11)
  language     String?   @db.VarChar(35)
  country      String?   @db.Char(2)
  subdivision1 String?   @db.Char(20)
  subdivision2 String?   @db.VarChar(50)
  city         String?   @db.VarChar(50)
  created_at   DateTime? @default(now()) @db.Timestamp(0)

  @@index([created_at])
  @@index([website_id, created_at, browser])
  @@index([website_id, created_at, city])
  @@index([website_id, created_at, country])
  @@index([website_id, created_at, device])
  @@index([website_id, created_at, hostname])
  @@index([website_id, created_at])
  @@index([website_id, created_at, language])
  @@index([website_id, created_at, os])
  @@index([website_id, created_at, screen])
  @@index([website_id, created_at, subdivision1])
  @@index([website_id])
}

model session_data {
  session_data_id String    @id @db.VarChar(36)
  website_id      String    @db.VarChar(36)
  session_id      String    @db.VarChar(36)
  event_key       String    @db.VarChar(500)
  string_value    String?   @db.VarChar(500)
  number_value    Decimal?  @db.Decimal(19, 4)
  date_value      DateTime? @db.Timestamp(0)
  data_type       Int       @db.UnsignedInt
  created_at      DateTime? @default(now()) @db.Timestamp(0)

  @@index([created_at])
  @@index([session_id])
  @@index([website_id])
}

model team {
  team_id     String    @id @unique @db.VarChar(36)
  name        String    @db.VarChar(50)
  access_code String?   @unique @db.VarChar(50)
  created_at  DateTime? @default(now()) @db.Timestamp(0)
  updated_at  DateTime? @db.Timestamp(0)
  deleted_at  DateTime? @db.Timestamp(0)
  logo_url    String?   @db.VarChar(2183)

  @@index([access_code])
}

model team_user {
  team_user_id String    @id @unique @db.VarChar(36)
  team_id      String    @db.VarChar(36)
  user_id      String    @db.VarChar(36)
  role         String    @db.VarChar(50)
  created_at   DateTime? @default(now()) @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)

  @@index([team_id])
  @@index([user_id])
}

model user {
  user_id      String    @id @unique @db.VarChar(36)
  username     String    @unique @db.VarChar(255)
  password     String    @db.VarChar(60)
  role         String    @db.VarChar(50)
  created_at   DateTime? @default(now()) @db.Timestamp(0)
  updated_at   DateTime? @db.Timestamp(0)
  deleted_at   DateTime? @db.Timestamp(0)
  display_name String?   @db.VarChar(255)
  logo_url     String?   @db.VarChar(2183)
}

model website {
  website_id String    @id @unique @db.VarChar(36)
  name       String    @db.VarChar(100)
  domain     String?   @db.VarChar(500)
  share_id   String?   @unique @db.VarChar(50)
  reset_at   DateTime? @db.Timestamp(0)
  user_id    String?   @db.VarChar(36)
  created_at DateTime? @default(now()) @db.Timestamp(0)
  updated_at DateTime? @db.Timestamp(0)
  deleted_at DateTime? @db.Timestamp(0)
  created_by String?   @db.VarChar(36)
  team_id    String?   @db.VarChar(36)

  @@index([created_at])
  @@index([created_by])
  @@index([share_id])
  @@index([team_id])
  @@index([user_id])
}

model website_event {
  event_id        String    @id @db.VarChar(36)
  website_id      String    @db.VarChar(36)
  session_id      String    @db.VarChar(36)
  created_at      DateTime? @default(now()) @db.Timestamp(0)
  url_path        String    @db.VarChar(500)
  url_query       String?   @db.VarChar(500)
  referrer_path   String?   @db.VarChar(500)
  referrer_query  String?   @db.VarChar(500)
  referrer_domain String?   @db.VarChar(500)
  page_title      String?   @db.VarChar(500)
  event_type      Int       @default(1) @db.UnsignedInt
  event_name      String?   @db.VarChar(50)
  visit_id        String?   @db.VarChar(36)

  @@index([created_at])
  @@index([session_id])
  @@index([website_id, created_at, event_name])
  @@index([website_id, created_at])
  @@index([website_id, created_at, page_title])
  @@index([website_id, created_at, referrer_domain])
  @@index([website_id, created_at, url_path])
  @@index([website_id, created_at, url_query])
  @@index([website_id])
  @@index([website_id, session_id, created_at])
}
